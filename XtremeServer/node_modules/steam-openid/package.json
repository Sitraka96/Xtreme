{
  "name": "steam-openid",
  "version": "1.1.1",
  "description": "Provides an interface for sign into 3d-party sites using Steam OpenID",
  "main": "lib/index",
  "scripts": {
    "clean": "rimraf lib",
    "test": "mocha --require @babel/polyfill --require @babel/register -r chai/register-should",
    "debug": "babel-node --nolazy --inspect-brk=9229 debug/index.js",
    "build": "cross-env BABEL_ENV=production babel src -d lib",
    "prepare": "npm test && npm run clean && npm run build"
  },
  "files": [
    "lib"
  ],
  "repository": "github:AlexOwl/steam-openid",
  "keywords": [
    "steam",
    "openid",
    "auth",
    "login",
    "oauth",
    "steamcommunity",
    "identity",
    "api"
  ],
  "author": "Alex Owl <AlexOwl@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexOwl/steam-openid/issues"
  },
  "homepage": "https://github.com/AlexOwl/steam-openid#readme",
  "dependencies": {
    "bluebird": "3",
    "cheerio": "^1.0.0-alpha",
    "dotenv": "^6.2.0",
    "request": "request/request#670b563",
    "request-promise": "4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "flow-bin": "^0.92.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "randomatic": "^3.1.1",
    "rimraf": "^2.6.3",
    "steamcommunity": "^3.38.0"
  }
}
